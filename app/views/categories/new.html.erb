<h1> Make a Category </h1>


<div class="row">
  <div class="col-md-4">
    <p> Create a custom Category </p>
  </div>
  <div class="col-md-8">
    <%= form_for @category do |f| %>
      <div class="form-group"?
        <%= f.label :title %>
        <%= f.text_field :title %>
      </div>
      <div class="form-group">
        <%= f.submit "Save" %>
      </div>
    <% end %>
  </div>
</div>
